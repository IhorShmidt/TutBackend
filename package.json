{
  "name": "TutBackend",
  "version": "0.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "nodemon ./app/",
    "test": "set NODE_ENV=test ./node_modules/.bin/mocha test/** --recursive",
    "cover": "set NODE_ENV=test ./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha -- -R spec test/** --recursive",
    "jslinter": "./node_modules/.bin/eslint app/** test/**",
    "apidoc": "./node_modules/apidoc/bin/apidoc -i app/ -o apidoc/",
    "postinstall": "npm run cover && npm run apidoc"
  },
  "dependencies": {
    "aws-sdk": "^2.6.10",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "joi": "^9.2.0",
    "jsonwebtoken": "^7.1.9",
    "lodash": "^4.17.2",
    "mongoose": "^4.6.3",
    "morgan": "^1.7.0",
    "node-ses": "^2.0.1",
    "pug": "^2.0.0-beta6"
  },
  "devDependencies": {
    "apidoc": "^0.16.1",
    "async": "^2.1.2",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "eslint": "^3.7.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.2",
    "mongodb": "^2.2.10",
    "sinon": "^1.17.6",
    "underscore": "^1.8.3"
  }
}
